/* axicomponent/bar/axi-tabbar.wxss */
axi-tabbar{
  display: flex;
  width: 100%;
  height: 60px;
}

axi-tabbar > view{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100%;
}

axi-tabbar .private-axi-tabbar-item{
  flex: 1;
}

axi-tabbar .private-axi-tabbar-item > view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

axi-tabbar .private-axi-tabbar-item  axi-image{
  width: 32px;
  height: 32px;
}