/* components/RefreshLayout/refresh-layout.wxss */
axi-refresh{
  display: flex;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  position: relative;
}

.refresh-layout-bottom{
  padding: 20px;
}

/* components/RefreshLayout/refresh-layout.wxss */

.refresh-container {
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}
.refresh-layout {
  display: block;
  vertical-align: center;
  width: 100%;
  text-align: center;
  align-self: flex-end;
}
.refresh-down{
  background: transparent url(data:image/png;base64,) no-repeat;
}
.refresh-up{
  background: transparent url(data:image/png;base64,) no-repeat;
}
.refresh-loading{
  background: transparent url(data:image/gif;base64,) no-repeat;
}
.refresh-status {
  margin: 0 5px;
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
  vertical-align: middle;
  /* -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite; */
  
  background-size: 100%;
}

.refresh-tips {
  color: #666;
  display: inline;
  line-height: 30rpx;
  font-size: 26rpx;
  vertical-align: middle;
}

.refresh-content{
  display: flex;
  flex-direction: column;
}
